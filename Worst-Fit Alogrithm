#include <stdio.h>

void worstFit(int b[], int m, int p[], int n) {
    int alloc[n];
    for (int i = 0; i < n; i++) {
        int worst = -1;
        for (int j = 0; j < m; j++)
            if (b[j] >= p[i] && (worst == -1 || b[j] > b[worst]))
                worst = j;
        alloc[i] = (worst != -1) ? (b[worst] -= p[i], worst + 1) : -1;
    }
    for (int i = 0; i < n; i++)
        printf("P%d %s B%d\n", i + 1, alloc[i] == -1 ? "-> Not Allocated" : "->", alloc[i]);
}

int main() {
    int b[] = {100, 500, 200, 300, 600}, p[] = {212, 417, 112, 426};
    worstFit(b, 5, p, 4);
    return 0;
}
